configuration {
	modi: "window,windowcd,drun";
	width: 1400;
	lines: 10;
	columns: 2;
	font: "Ubuntu Mono 24";
	location: 2;
	yoffset: 400;
	show-icons: true;
	parse-known-hosts: false;
	hide-scrollbar: true;
	window-format: " {t}";
	theme: "dotsanity";
	display-window: "clients";
	display-windowcd: "tag";
	display-drun: "run";
}
